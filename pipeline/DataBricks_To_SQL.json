{
	"name": "DataBricks_To_SQL",
	"properties": {
		"activities": [
			{
				"name": "Delta_To_SQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "hemanth.parquet_table"
					},
					{
						"name": "Destination",
						"value": "."
					}
				],
				"typeProperties": {
					"source": {
						"type": "AzureDatabricksDeltaLakeSource",
						"exportSettings": {
							"type": "AzureDatabricksDeltaLakeExportCommand"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"copyBehavior": "FlattenHierarchy"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".csv"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "AzureDatabricksDeltaLakeDataset1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DataBricksExtraction",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}