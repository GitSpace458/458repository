{
	"name": "ParameterizedDataSet",
	"properties": {
		"description": "This is the parametrized Dataset",
		"linkedServiceName": {
			"referenceName": "ParameterizedTesting",
			"type": "LinkedServiceReference",
			"parameters": {
				"StorageAccountName": "hemanthstorageaccount",
				"EndSuffixName": "core.windows.net"
			}
		},
		"parameters": {
			"ContainerName": {
				"type": "string"
			},
			"DirectoryName": {
				"type": "string"
			},
			"FileName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@dataset().FileName",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().DirectoryName",
					"type": "Expression"
				},
				"container": {
					"value": "@dataset().ContainerName",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}